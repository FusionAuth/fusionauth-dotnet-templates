{
  "DetailedErrors": true,
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Authentication": {
    "Schemes": {
      "FusionAuth": {
        "RequireHttpsMetadata": "false"
      }
    }
  },
  "FusionAuth": {
    "ClientSecret": "{CLIENTSECRET}"
  },
  "Kestrel": {
    "Endpoints": {
      "Https": {
        "Url": "https://localhost:{PORT}"
      }
    }
  }
}

